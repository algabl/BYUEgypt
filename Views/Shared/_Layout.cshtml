<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - BYUEgypt</title>
    <link rel="icon" type="image/x-icon" href="~/lib/favicon.ico"/>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/BYUEgypt.styles.css" asp-append-version="true"/>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@300&family=El+Messiri&family=Montserrat:ital,wght@0,200;0,300;0,500;1,400&family=Sanchez&display=swap" rel="stylesheet">
</head>
<body>
<header>
    <nav class="navbar navbar-expand-lg bg-body-tertiary bg-primary text-light box-shadow nav-border-bottom cairo-font">
      <div class="container-fluid">
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>
          <div class="collapse navbar-collapse" id="navbarNavDropdown">
          <a class="navbar-brand text-light" asp-area="" asp-controller="Home" asp-action="Index">BYU<b>Egypt</b></a>
            <ul class="navbar-nav">
              <li class="nav-item navbar-text me-auto mb-2 mb-lg-0">
                <a class="nav-link text-light active" aria-current="page" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
              </li>
              <li class="nav-item navbar-text">
                <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
              </li>
              <li class="nav-item navbar-text">
                <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="">Users</a>
              </li>
         
              <li class="nav-item navbar-text dropdown">
                <a class="nav-link dropdown-toggle text-light" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                  Record View
                </a>
                  <ul class="dropdown-menu bg-primary">
                    <li><a class="dropdown-item text-light" asp-area="" asp-controller="Home" asp-action="EditRecord">Edit Record</a></li>
                    <li><a class="dropdown-item text-light" asp-area="" asp-controller="Home" asp-action="RecordView">Record Public</a></li>
                  </ul>
              </li>
              <li class="nav-item navbar-text dropdown">
                <a class="nav-link dropdown-toggle text-light" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                  Analysis
                </a>
                <ul class="dropdown-menu bg-primary">
                  <li><a class="dropdown-item text-light" asp-area="" asp-controller="Analysis" asp-action="SupAnalysis">Supervised Analysis</a></li>
                  <li><a class="dropdown-item text-light" asp-area="" asp-controller="Analysis" asp-action="UnSupAnalysis">Unsupervised Analysis</a></li>
                </ul>
              </li>
              </ul>
            <div class="d-flex w-100 justify-content-end">
              <div class="me-5">
                <partial name="_LoginPartial"/>
              </div>
              
            </div>
          </div>
        </div>
    </nav>
</header>
<div class="">
        <partial name="_CookieConsentPartial" />
    <main role="main" class="pb-3">

        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-secondary">
    <div class="container">
        &copy; 2023 - BYUEgypt - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>