        content="
                script-src 'sha256-48j60cIjTy/7qzSLIyAyJkrX6k4X0KtE5FU3r12fXrI=' 'sha256-CISjryGlszPm9TFgRqfA3V7Kga2UPDZTAqbSvDlZvmg='
